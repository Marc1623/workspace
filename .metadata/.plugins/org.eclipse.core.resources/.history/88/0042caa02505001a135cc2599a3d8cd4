
public class Ex3 {

	
	private int nombre=0;
    private int [] tableau;
    private int temp=0;
    private int index=0;
    
    public Ex3 (int nombre) {
    	this.nombre=nombre;
        tableau = new int[nombre];

        for (int i=0;i<tableau.length;i++){
            tableau[i] =(int)(Math.random()*10) + 1;
        }
    }
    
    public void affiche(){

        for (int i=0;i<tableau.length;i++){
            System.out.print(tableau[i]+"   ");
        }

        System.out.println();
    }
    
}
